{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
	{
	    "type": "prompt",
	    "alignment": "left",
	    "segments": [
		{
		    "properties": {
			"cache_duration": "none"
		    },
		    "template": " âš¡",
		    "foreground": "#dd1e1e",
		    "background": "#000000",
		    "type": "root",
		    "style": "plain"
		},
		{
		    "properties": {
			"alpine": "\uf300",
			"arch": "\uf303",
			"cache_duration": "none",
			"centos": "\uf304",
			"debian": "\uf306",
			"elementary": "\uf309",
			"fedora": "\uf30a",
			"gentoo": "\uf30d",
			"linux": "\ue712",
			"macos": "\ue711",
			"manjaro": "\uf312",
			"mint": "\uf30f",
			"opensuse": "\uf314",
			"raspbian": "\uf315",
			"ubuntu": "\uf31c",
			"windows": "\ue62a",
			"wsl": "\ue712"
		    },
		    "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
		    "foreground": "#ffffff",
		    "background": "transparent",
		    "type": "os",
		    "style": "plain"
		},
		{
		    "properties": {
			"cache_duration": "none"
		    },
		    "leading_diamond": "\u2591",
		    "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }} ",
		    "foreground": "#ffffff",
		    "background": "#ea3d54",
		    "type": "session",
		    "style": "diamond"
		},
		{
		    "properties": {
			"cache_duration": "none",
			"style": "letter"
		    },
		    "foreground": "#ffffff",
		    "background": "#3f444a",
		    "type": "path",
		    "style": "diamond"
		},
		{
		    "properties": {
			"cache_duration": "none",
			"fetch_status": true,
			"fetch_upstream_icon": true,
			"untracked_modes": {
			    "/Users/user/Projects/oh-my-posh/": "no"
			}
		    },
		    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }} ",
		    "foreground": "#193549",
		    "powerline_symbol": "\ue0b0",
		    "background": "#5689f0",
		    "type": "git",
		    "style": "powerline",
		    "background_templates": [
			"{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
			"{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
			"{{ if gt .Ahead 0 }}#B388FF{{ end }}",
			"{{ if gt .Behind 0 }}#B388FB{{ end }}"
		    ]
		}
	    ]
	},
	{
	    "type": "rprompt",
	    "alignment": "right",
	    "segments": [
		{
		    "properties": {
			"cache_duration": "none",
			"time_format": "15:04:05"
		    },
		    "foreground": "#ffffff",
		    "type": "time",
		    "style": "plain"
		},
		{
		    "properties": {
			"always_enabled": true,
			"cache_duration": "none",
			"style": "austin",
			"threshold": 500
		    },
		    "template": " <#fefefe>\ueba2</> {{ .FormattedMs }} ",
		    "foreground": "#ffffff",
		    "type": "executiontime",
		    "style": "plain"
		}
	    ]
	}
    ],
    "final_space": true,
    "version": 3
}
